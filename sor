def bubble_sort(nums):
    n = len(nums)
    for i in range(n - 1):
        for j in range(n - 1 - i):
            if nums[j] > nums[j + 1]:
                # Swap the elements
                nums[j], nums[j + 1] = nums[j + 1], nums[j]

# Example usage
numbers = [5, 2, 9, 1, 7]
print("Original list:", numbers)

bubble_sort(numbers)
print("Sorted list:", numbers)
